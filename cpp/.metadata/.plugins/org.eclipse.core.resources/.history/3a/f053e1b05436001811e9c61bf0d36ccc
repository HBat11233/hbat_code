#include <iostream>
#include <ctime>

using namespace std;

class times
{
public:
	times(int num=0,int nd=0)
		:nd(nd)
	{
		this->num=1;
	}
	int getNum()
	{
		return this->num;
	}
private:
	static int num;
	int nd;
};

int main()
{
	int num=0;
	int &a(num);
	cout<<a<<' '<<num;
	times d;
	cout<<d.getNum();
	return 0;
}


