#include <stdio.h>

int n,m;
int MMS[100001][3];
int Mlength=0;
int cl[100001];
int bt[100001];

int main()
{
	scanf("%d%d\n",&n,&m);
	for(int i=0;i<m;++i)
	{
		char ch;
		scanf("%c",&ch);
		if(ch=='L')
		{
			scanf("%d%d%d\n",&MMS[Mlength][0],&MMS[Mlength][1],&MMS[Mlength][2]);
			Mlength++;
		}else
		{
			int tenum;
			scanf("%d\n",&tenum);
			for(int j=cl[tenum]?bt[tenum]:0;j<Mlength;++j)
				if(MMS[j][0]<=tenum&&MMS[j][1]>=tenum)
					cl[tenum]+=MMS[j][2];
			bt[tenum]=Mlength;
			printf("%d\n",cl[tenum]+100);
		}
	}
	return 0;
}
