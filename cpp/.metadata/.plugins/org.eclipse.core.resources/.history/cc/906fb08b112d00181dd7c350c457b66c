#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int n,m;
char ch;
struct Ls
{
	int a;
	int b;
	int c;
	Ls(int a,int b,int c)
		:a(a),b(b),c(c){}
	bool operator <(const Ls&A)
	{
		return a<A.a;
	}
};
vector<Ls>que;

int main()
{
	cin>>n>>m;
	for(int i=0;i<m;++i)
	{
		cin>>ch;
		int a,b,c;
		if(ch=='L')
		{
			cin>>a>>b>>c;
			que.push_back(Ls(a,b,c));
		}else
		{
			int tempnum;
			cin>>tempnum;
			sort(que.begin(),que.end());
			int ans=0;
			for(int i=0;i<que.size();++i)
			{
				if(que[i].a>tempnum)break;
				if(que[i].b<tempnum)continue;
				ans+=que[i].c;
			}
			cout<<ans+100<<endl;
		}
	}
	return 0;
}
