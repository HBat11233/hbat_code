#include <stdio.h>
#include <vector>

using namespace std;

struct LS
{
	int a;
	int b;
	int c;
	LS(int a=0,int b=0,int c=0):
		a(a),b(b),c(c){}
};

struct BS
{
	int s;//²éÑ¯
	int t;//Â¥²ã
	BS(int s=0,int t=0):
		s(s),t(t){}
};
int n,m;
vector<LS>MMS;
vector<BS>AAS;

int main()
{
	scanf("%d%d\n",&n,&m);
	for(int i=0;i<m;++i)
	{
		char ch;
		scanf("%c",&ch);
		if(ch=='L')
		{
			int a,b,c;
			scanf("%d%d%d\n",&a,&b,&c);
			MMS.push_back(LS(a,b,c));
		}else
		{
			int tenum;
			scanf("%d\n",&tenum);
			AAS.push_back( BS(tenum,MMS.size()) );
		}
	}
	return 0;
}
