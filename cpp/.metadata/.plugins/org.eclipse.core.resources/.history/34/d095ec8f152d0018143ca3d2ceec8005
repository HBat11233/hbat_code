#include <iostream>
#include <vector>
#include <cstdio>

using namespace std;

struct LS
{
	int a;
	int b;
	int c;
	LS(int a=0,int b=0,int c=0)
	:a(a),b(b),c(c){}
};

int n,m;
vector<LS>que;
int cl[100001];
int main()
{
	scanf("%d%d",&n,&m);
	for(int i=0;i<m;++i)
	{
		char ch;
		scanf("%c",&ch);
		if(ch=='L')
		{
			int a,b,c;
			scanf("%d%d%d",&a,&b,&c);
			for(int j=a;j<=b;++j)
				cl[j]+=c;
		}else
		{
			int tenum;
			scanf("%d",&tenum);
			printf("%d\n",cl[tenum]+100);
		}
	}
	return 0;
}
